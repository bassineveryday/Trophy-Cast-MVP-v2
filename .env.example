# Trophy Cast Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
# Replace the placeholders below with your own Supabase project URL and anon key
# Copy this file to `.env.local` and fill in real credentials for local development.
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Dev-only convenience: set to 'true' to force dev login, 'false' to force real auth
EXPO_PUBLIC_DEV_LOGIN=false

# Sentry Configuration (Optional - for error tracking in production)
SENTRY_DSN=your_sentry_dsn_here

# Development Configuration
NODE_ENV=development

# App Configuration
APP_VERSION=1.0.0